	"license": "MIT",
	"devDependencies": {
		"@tsconfig/svelte": "^5.0.4",
		"@types/node": "^24.0.1",
		"@typescript-eslint/eslint-plugin": "8.34.0",
		"@typescript-eslint/parser": "8.34.0",
		"esbuild-svelte": "^0.9.3",
		"builtin-modules": "5.0.0",
		"esbuild": "0.25.5",
		"obsidian": "latest",
		"svelte-check": "^4.2.1",
		"svelte-preprocess": "^6.0.3",
		"prettier": "3.5.3",
		"tslib": "2.8.1",
		"typescript": "^5.8.3"
	}
